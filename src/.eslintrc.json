{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        // no dangling
        "no-underscore-dangle": "off",
        // class methode can be used without this
        "class-methods-use-this": "off"
    }
}
